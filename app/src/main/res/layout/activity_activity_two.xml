<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.mini_.helloworld.ActivityTwo">

    <TextView
        android:text="@string/onCreateStr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mTv_Create"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"

        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"/>


    <TextView
        android:text="@string/onStartStr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mTv_Start"
        android:layout_alignParentTop="true"
        android:layout_marginTop="40dp"

        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"/>

    <TextView
        android:text="@string/onResumeStr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mTv_Resume"
        android:layout_alignParentTop="true"
        android:layout_marginTop="70dp"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"/>

    <TextView
        android:text="@string/onRestartStr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mTv_Restart"
        android:layout_alignParentTop="true"
        android:layout_marginTop="100dp"

        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Close Application"
        android:id="@+id/returnToActivity1"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"
        android:onClick="returnToActivityOne" />

</RelativeLayout>
